<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>SayHello</servlet-name> 
		<servlet-class>test.web.SayHello</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SayHello</servlet-name>
		<url-pattern>/SayHello</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SayHelloInternationalized</servlet-name> 
		<servlet-class>test.web.SayHelloInternationalized</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SayHelloInternationalized</servlet-name>
		<url-pattern>/SayHelloInternationalized</url-pattern>
	</servlet-mapping>
